/**
 *
 * @author Edwin Etornam
 */
public class XsAndYs {
    public static void main(String[] args) {
        System.out.println("x\t y");
        System.out.println("-------------------");
        double x;
        
        for (x = -10; x <= 10; x=x+0.5) {
            double y=x*x;
            System.out.println(x+"\t"+y);
            
            
            }
            
        }
    }
    

